        $myform = $('form');
        var rows = $('.printouttable').children('tbody').children('tr');
        var lines='';
        $.each(rows, function( index, value ) {
            var cols = $(this).children('td');
           var  question = $(cols[0]).text();
           if (question.trim()!='' && (typeof cols[1] === 'undefined')) {
               question = question.toUpperCase();
           } else
           {
               if (question.trim()!=''){
                   question = escape("-")+question;
               }
           }
           var answer = cols[1]?escape(":")+escape("\n")+' '+$(cols[1]).text():'';
            lines = lines + question + answer+escape("\n"); 
        });
 

        $("strong").remove();
        var mytitle = $('.printouttitle').text();
        var titlesurvey = $('.printouttitle').html();

        $myform.prop ('action','mailto:{MAILTOSENDANSWERS}?body='+textoIntroductorio+lines+'&subject=Respuestas a la encuesta: '+titlesurvey);
        $myform.submit();
     }
